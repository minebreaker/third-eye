{
    "name": "third-eye",
    "version": "0.1.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "npm run assemble && npm run test",
        "build:production": "export NODE_ENV=production && npm run clean && npm run build",
        "assemble": "npm run compile && npm run bundle && npm run copy",
        "run": "electron build/out/index.js",
        "run:refresh": "npm run assemble && npm run run",
        "compile": "npm run compileClient && npm run compileServer",
        "compileClient": "tsc -p tsconfig.client.json",
        "compileServer": "tsc -p tsconfig.server.json",
        "bundle": "webpack --display-error-details",
        "copy": "npm run copyStatic && npm run copyServer",
        "copyStatic": "cpx static/index.html build/out",
        "copyServer": "cpx build/sources/src/server/* build/out/server && cpx build/sources/src/shared/* build/out/shared && cpx build/sources/src/index.js build/out",
        "test": "mocha --config .mocharc.js",
        "clean": "rimraf ./build/*",
        "serve": "cd build/out && python -m http.server",
        "watchCompile": "tsc -p tsconfig.json --incremental -w",
        "watchBundle": "webpack -w"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "lodash": "^4.17.16",
        "luxon": "^1.24.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-jss": "^10.3.0",
        "react-redux": "^7.2.0",
        "redux": "^4.0.5",
        "redux-devtools-extension": "^2.13.8",
        "redux-thunk": "^2.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.10.4",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/polyfill": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/register": "^7.10.4",
        "@types/chai": "^4.2.11",
        "@types/enzyme": "^3.10.5",
        "@types/lodash": "^4.14.157",
        "@types/luxon": "^1.24.1",
        "@types/mocha": "^7.0.2",
        "@types/react": "^16.9.41",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.9",
        "@types/react-test-renderer": "^16.9.2",
        "babel-loader": "^8.1.0",
        "chai": "^4.2.0",
        "cpx": "^1.5.0",
        "electron": "^9.3.1",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "mocha": "^8.0.1",
        "nyc": "^15.1.0",
        "rimraf": "^3.0.2",
        "source-map-support": "^0.5.19",
        "ts-loader": "^7.0.5",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.6",
        "webpack": "^4.43.0",
        "webpack-cli": "^3.3.12"
    }
}
